         <rowConstraints>
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
         </rowConstraints>
        </GridPane>
        <Button layoutX="373.0" layoutY="380.0" onAction="#btnActualizarClick" prefHeight="25.0" prefWidth="101.0" style="-fx-background-color: #146cd0;" text="Actualizar" textFill="WHITE">
            <font>
                <Font name="Arial Bold" size="14.0" />
            </font>
        </Button>
        <Button layoutX="491.0" layoutY="380.0" onAction="#btnCancelarClick" prefHeight="25.0" prefWidth="80.0" style="-fx-background-color: #6c757d;" text="Cancelar" textFill="WHITE">
            <font>
                <Font name="Arial Bold" size="14.0" />
            </font>
        </Button>
    </children>
