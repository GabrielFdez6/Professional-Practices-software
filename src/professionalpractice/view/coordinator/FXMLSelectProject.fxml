<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="professionalpractice.controller.coordinator.FXMLSelectProjectController">
    <children>
        <Label layoutX="225.0" layoutY="24.0" text="Seleccionar Proyecto">
            <font><Font name="System Bold" size="18.0" /></font>
        </Label>
        <TableView fx:id="tvProjects" layoutX="25.0" layoutY="60.0" prefHeight="280.0" prefWidth="550.0">
            <columns>
                <TableColumn fx:id="colProjectName" prefWidth="275.0" text="Nombre del Proyecto" />
            </columns>
        </TableView>
        <Button layoutX="420.0" layoutY="350.0" mnemonicParsing="false" onAction="#btnSeleccionarClick" text="Seleccionar" />
        <Button layoutX="515.0" layoutY="350.0" mnemonicParsing="false" onAction="#btnCancelarClick" text="Cancelar" />
    </children>
</AnchorPane>
